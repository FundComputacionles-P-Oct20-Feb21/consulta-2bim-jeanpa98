Algoritmo ejercicio05
	Escribir "Operacion de tres números: ";
	Escribir "Ingrese el primer valor";
	Leer valor1;
	Escribir "Ingrese el segundo valor";
	Leer valor2;
	Escribir "Ingrese el tercer valor";
	Leer valor3;
	Escribir "Piensa: La operación valor1*valor2*valor3/valor1";
